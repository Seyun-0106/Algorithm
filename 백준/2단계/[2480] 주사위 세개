A,B,C = map(int, input().split())
list = [A,B,C]

# 같은 눈 3개
if(list.count(A)==3):
    prize = 10000+A*1000
    print(prize)
# 같은 눈 0개
elif(list.count(A)==1 and list.count(B)==1):
    prize = max(list)*100
    print(prize)
else:
    # 같은 눈 2개
    for i in list:
        if(list.count(i)==2):
            prize = 1000+i*100
            print(prize)
            break
